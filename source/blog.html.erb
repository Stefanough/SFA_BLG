<% blog('scribbles').articles.each_with_index do |article, i| %>
    <% if !["Welcome", "The Jargon File"].include? article.title %>
        <h2><%= link_to article.title, article %> <span><%= article.date.strftime('%b %e') %></span></h2>
    <% end %>
<% end %>
  
